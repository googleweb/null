<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="edittext_style" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/selector_edittext_bg</item>
    </style>

    <style name="inspection_text">
        <item name="android:textColor">#000099</item>
        <item name="android:textSize">18dp</item>
        <item name="android:height">22dp</item>
    </style>
    <style name="inspection_text_chk">
        <item name="android:textColor">#000099</item>
        <item name="android:textSize">18dp</item>
        <item name="android:height">33dp</item>
    </style>
    <style name="dialog_text">
        <item name="android:textSize">15dp</item>
        <item name="android:height">22dp</item>
    </style>
    
    <style name="inspection_value">
        <item name="android:textColor">#000000</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18dp</item>
    </style>
    
   	<style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    
    <style name="inspection_radioButton">
        <item name="android:textColor">#486e9a</item>
        <item name="android:textSize">14dp</item>
    </style>

    <style name="inspection_item_title">
        <item name="android:textSize">26px</item>
        <item name="android:paddingLeft">25px</item>
        <item name="android:paddingTop">12px</item>
        <item name="android:height">55px</item>
        <item name="android:width">720px</item>
        <item name="android:textColor">#5990c7</item>
        <item name="android:background">@drawable/tbar_drop</item>
    </style>

  

  

    <style name="radio_btn_style1">
        <item name="android:textColor">@drawable/title_color</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18dp</item>
        <item name="android:background">@drawable/radio_btn_style1</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
    </style>
    
    <style name="radio_btn_style2">
        <item name="android:textColor">@drawable/title_color</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18dp</item>
        <item name="android:background">@drawable/radio_btn_style2</item>
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="inspection_select_title">
        <item name="android:textColor">#5990c7</item>
        <item name="android:textSize">20dp</item>
    </style>

</resources>